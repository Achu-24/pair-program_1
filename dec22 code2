def assignCookies(student, cookie):
    student.sort()
    cookie.sort()

    i = 0  
    j = 0  
    count = 0

    while i < len(student) and j < len(cookie):
        if cookie[j] >= student[i]:
            count += 1
            i += 1
            j += 1
        else:
            j += 1

    return count



student = list(map(int, input("Enter student greed factors: ").split()))
cookie = list(map(int, input("Enter cookie sizes: ").split()))

print(assignCookies(student, cookie))
